<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="navPane" fillWidth="false" prefHeight="410.0" prefWidth="150.0" style="-fx-border-width: 5; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.NavController">
   <children>
      <Button fx:id="homeNavBtn" alignment="CENTER" layoutX="10.0" layoutY="75.0" mnemonicParsing="false" onAction="#goToHome" prefHeight="40.0" prefWidth="160.0" text="Home">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="15.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="searchNavBtn" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onAction="#goToSearch" prefHeight="40.0" prefWidth="160.0" text="Search">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="15.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="contactNavBtn" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onAction="#goToContact" prefHeight="40.0" prefWidth="160.0" text="Contact">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="15.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="signOutNavBtn" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onAction="#signOut" prefHeight="40.0" prefWidth="160.0" text="Sign Out">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="180.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
